    "extra": {
        "laravel": {
            "providers": [
                "almosoft\\widgetmanager\\widgetmanagerServiceProvider"
            ],
            "aliases": {
                "widgetmanager": "almosoft\\widgetmanager\\Facades\\widgetmanager"
