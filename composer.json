{
    "name": "almosoft/widgetmanager",
    "description": ":Widget manager for backpackforlaravel",
    "version":"1.0.0",
    "authors": [
        {
            "name": "Aleksander",
            "email": "register1705@yandex.ru",
            "homepage": "https://github.com/Aleksandr1705/widgetmanager"
        }
    ],
    "homepage": "https://github.com/almosoft/widgetmanager",
    "keywords": ["Laravel", "widgetmanager"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "almosoft\\widgetmanager\\": "src/",
            "almosoft\\widgetmanager\\Database\\Seeds\\":"src/database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "almosoft\\widgetmanager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "almosoft\\widgetmanager\\widgetmanagerServiceProvider"
            ],
            "aliases": {
                "widgetmanager": "almosoft\\widgetmanager\\Facades\\widgetmanager"
            }
        }
    }
}
